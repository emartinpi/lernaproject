    "lerna": "^3.1.4",
    "ora": "^3.0.0",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-generate-package-json": "^2.1.1",
    "rollup-plugin-json": "^3.0.0",
