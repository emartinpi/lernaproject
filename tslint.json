{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "class-name": true,
    "no-any": false,
    "no-unused-variable": true,
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": [true, 150]
  },
  "rulesDirectory": []
}
